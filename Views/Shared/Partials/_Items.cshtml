@* Fetch API loadProducts sản phẩm *@
<script>
    document.addEventListener("DOMContentLoaded", () => {
    fetchProducts();
});
async function fetchProducts() {
    try {
        const response = await fetch("/api/Product");
        if (!response.ok) {
            throw new Error("Lỗi khi lấy dữ liệu từ API");
        }

        const products = await response.json();
        renderProducts(products);
        
        // Sau khi render xong sản phẩm, tạo nút điều khiển slider
        const buttonContainerPC = document.getElementById("button-container-item");
        const buttonContainerLaptop = document.getElementById("button-container-item-laptop");
        const buttonContainerMouse = document.getElementById("button-container-item-mouse");
        const buttonContainerKeyboard = document.getElementById("button-container-item-keyboard");
        const buttonContainerMonitor = document.getElementById("button-container-item-monitor");

        createSliderControls(buttonContainerPC, '#product-list', 'slider-prev', 'slider-next');  // Gọi hàm tạo nút điều khiển cho PC
        createSliderControls(buttonContainerLaptop, '#product-list-laptop', 'slider-prev2', 'slider-next2');  // Gọi hàm tạo nút điều khiển cho Laptop
        createSliderControls(buttonContainerMouse, '#product-list-mouse', 'slider-prev3', 'slider-next3');  // Gọi hàm tạo nút điều khiển cho Mouse
        createSliderControls(buttonContainerKeyboard, '#product-list-keyboard', 'slider-prev4', 'slider-next4');  // Gọi hàm tạo nút điều khiển cho Keyboard
        createSliderControls(buttonContainerMonitor, '#product-list-monitor', 'slider-prev5', 'slider-next5');  // Gọi hàm tạo nút điều khiển cho Monitor


    } catch (error) {
        console.error("Lỗi:", error);
    }
}


function renderProducts(products) {
    // Lấy danh sách container hiển thị sản phẩm
    const productContainerPC = document.getElementById("product-list");
    const productContainerLaptop = document.getElementById("product-list-laptop");
    const productContainerMouse = document.getElementById("product-list-mouse");
    const productContainerKeyboard = document.getElementById("product-list-keyboard");
    const productContainerMonitor = document.getElementById("product-list-monitor");

    if (!productContainerPC || !productContainerLaptop || !productContainerMouse || !productContainerKeyboard || !productContainerMonitor) {
        console.error("Không tìm thấy phần tử danh sách sản phẩm!");
        return;
    }

    // Xóa nội dung cũ trước khi hiển thị
    productContainerPC.innerHTML = `<div class="product-slider-wrapper"></div>`;
    productContainerLaptop.innerHTML = `<div class="product-slider-wrapper"></div>`;
    productContainerMouse.innerHTML = `<div class="product-slider-wrapper"></div>`;
    productContainerKeyboard.innerHTML = `<div class="product-slider-wrapper"></div>`;
    productContainerMonitor.innerHTML = `<div class="product-slider-wrapper"></div>`;

    const productSliderPC = productContainerPC.querySelector(".product-slider-wrapper");
    const productSliderLaptop = productContainerLaptop.querySelector(".product-slider-wrapper");
    const productSliderMouse = productContainerMouse.querySelector(".product-slider-wrapper");
    const productSliderKeyboard = productContainerKeyboard.querySelector(".product-slider-wrapper");
    const productSliderMonitor = productContainerMonitor.querySelector(".product-slider-wrapper");

    products.forEach(productDTO => {
        if (!productDTO || !productDTO.product) {
            console.error("Dữ liệu sản phẩm bị lỗi:", productDTO);
            return;
        }

        const { product, attributes, imageThumbnail } = productDTO;

        if (!product.productName) {
            console.error("Lỗi: productName không tồn tại", product);
            return;
        }

        // Xác định loại sản phẩm
        const isLaptop = product.loai && product.loai.toLowerCase() === "laptop";
        const isPC = product.loai && product.loai.toLowerCase() === "pc";
        const isMouse = product.loai && product.loai.toLowerCase() === "mouse";
        const isKeyboard = product.loai && product.loai.toLowerCase() === "keyboard";
        const isMonitor = product.loai && product.loai.toLowerCase() === "monitor";

        // Danh sách thuộc tính hiển thị dựa trên loại sản phẩm
        let requiredAttributes = [];
        if (isLaptop) {
            requiredAttributes = ["CPU", "VGA", "RAM", "SSD", "Màn hình", "Tần số quét"];
        } else if (isPC) {
            requiredAttributes = ["CPU", "VGA", "Mainboard", "RAM", "SSD"];
        } else if (isMouse) {
            requiredAttributes = ["Dây", "LED RGB", "DPI"];
        } else if (isKeyboard) {
            requiredAttributes = ["Loại bàn phím", "Switch", "Keycaps"];
        } else if (isMonitor) {
            requiredAttributes = ["Kích thước", "Độ phân giải", "Tần số quét", "Tấm nền"];
        }

        // Sử dụng trực tiếp URL từ API 
        let imgSrc = imageThumbnail ? imageThumbnail : "default-thumbnail.png";

        // Xử lý danh sách thông số kỹ thuật (chỉ hiển thị nếu có giá trị hợp lệ)
        const displayedAttributes = requiredAttributes.map(attrName => {
            const attr = attributes.find(a => a.productAttributeName === attrName);
            
            if (!attr || !attr.attributeSummary || attr.attributeSummary.trim() === "" || 
                attr.attributeSummary.toUpperCase() === "NULL" || attr.attributeSummary === "Chưa có") {
                return ""; // Bỏ qua nếu thuộc tính không có giá trị hợp lệ
            }

            let attrValue = attr.attributeSummary;

            // Lấy SVG theo loại sản phẩm
            const iconType = isLaptop ? "laptop" : isMouse ? "mouse" : isKeyboard ? "keyboard" : isMonitor ? "monitor" : "pc";

            return `
                <div class="spec-item-item">
                    <svg viewBox="0 0 10 10">${getSvgIcon(attrName, iconType)}</svg>
                    <span class="spec-text-item">${attrValue}</span>
                </div>
            `;
        }).filter(item => item !== "").join(""); // Loại bỏ các chuỗi rỗng trước khi ghép HTML


        // Tạo thẻ HTML cho sản phẩm
        const productCard = document.createElement("div");
        productCard.classList.add("product-card-item");
        productCard.onclick = () => {
            window.location.href = `/Home/Products?productID=${product.productID}`;
        };

        productCard.innerHTML = `
            <img src="${imgSrc}" alt="${product.productName}">
            <div class="product-title-item">${product.productName}</div>
            <div class="specs-container-item">${displayedAttributes}</div>
            <div class="product-price-item">
                <span class="old-price-item">${product.priceOriginal.toLocaleString()}đ</span>
                <div class="current-price-container-item">
                    <span class="current-price-item">${product.priceAfterDiscount.toLocaleString()}đ</span>
                    <span class="discount-item">-${product.discountPercentage || 0}%</span>
                </div>
            </div>
            <div class="product-rating-item">
                <span class="rating-item">5.0 ★</span>
                <span class="review-count-item">(4 đánh giá)</span>
            </div>
        `;

        // Thêm sản phẩm vào danh sách phù hợp
        if (isLaptop) {
            productSliderLaptop.appendChild(productCard);
        } else if (isPC) {
            productSliderPC.appendChild(productCard);
        } else if (isMouse) {
            productSliderMouse.appendChild(productCard);
        } else if (isKeyboard) {
            productSliderKeyboard.appendChild(productCard);
        } else if (isMonitor) {
            productSliderMonitor.appendChild(productCard);
        } else {
            console.warn("Sản phẩm không xác định loại:", product);
        }
    });
}

// Hàm lấy icon SVG
